[core]
  editor = ~/bin/vim
  ignorecase = false
[alias]
   l = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short
   a = add
   camend = commit --amend
   cm = commit -m
   co = checkout
   cob = checkout -b
   s = status -s
   d = diff
   po = push origin

